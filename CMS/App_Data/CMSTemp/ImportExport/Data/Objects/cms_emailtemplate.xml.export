    <cms_emailtemplate>
      <EmailTemplateID>166</EmailTemplateID>
      <EmailTemplateName>Ecommerce.OrderNotificationToCustomer</EmailTemplateName>
      <EmailTemplateDisplayName>E-commerce - Order notification to customer</EmailTemplateDisplayName>
      <EmailTemplateText>&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body&gt;
&lt;p&gt;Thank you for your order. Below you can find the order details.&lt;/p&gt;
&lt;table width="600" cellspacing="0" cellpadding="5" border="1" style="border-color:black;"&gt;
    &lt;tbody&gt;
        &lt;tr&gt;
            &lt;td height="50" valign="bottom" colspan="2"&gt;
            &lt;table width="100%"&gt;
                &lt;tbody&gt;
                    &lt;tr&gt;
                        &lt;td style="text-align: left; vertical-align: bottom;"&gt;&lt;span style="font-size: 18pt;"&gt;Your order&lt;/span&gt;&lt;/td&gt;
                        &lt;td style="text-align: center; vertical-align: middle;"&gt;&lt;span style="font-family: Garamond,Times,serif; font-size: 24pt; font-style: italic;"&gt;Company logo&lt;/span&gt;&lt;/td&gt;
                    &lt;/tr&gt;
                &lt;/tbody&gt;
            &lt;/table&gt;
            &lt;/td&gt;
        &lt;/tr&gt;
        &lt;tr&gt;
			&lt;td style="text-align: left; padding: 8px"&gt;
			&lt;table style="width: 287px; height: 23px"&gt;
				&lt;tbody&gt;
					&lt;tr&gt;
						&lt;td style="text-align: left; color:black; font-weight:bold;"&gt;Invoice number&lt;/td&gt;
						&lt;td style="text-align: right; padding-right: 10px; padding: 8px"&gt;{%HTMLEncode(Order.OrderInvoiceNumber) |(user)administrator|(hash)816fc70f63a9e36493f026a0fc912f8a4dfbb25dc829d64adf9bdedc679870e3%}&lt;/td&gt;
					&lt;/tr&gt;
				&lt;/tbody&gt;
			&lt;/table&gt;
			&lt;/td&gt;
			&lt;td style="text-align: left; padding: 8px"&gt;
			&lt;table width="100%"&gt;
				&lt;tbody&gt;
					&lt;tr&gt;
						&lt;td style="text-align: left; color:black; font-weight:bold;"&gt;Order date:&lt;/td&gt;
						&lt;td style="text-align: right; padding: 8px"&gt;{% GetCurrentDateTimeString(Format(Order.OrderDate, "{0:g}"), (Customer.CustomerUser == null) ? "" : Customer.CustomerUser.UserName) |(user)administrator|(hash)b69a9f4284f8de9301e9a7d0a045b6b094d5ca90ebae904e4f953ef21d406696%}&lt;/td&gt;
					&lt;/tr&gt;
				&lt;/tbody&gt;
			&lt;/table&gt;
			&lt;/td&gt;
		&lt;/tr&gt;
		&lt;tr&gt;
			&lt;td style="text-align: left; vertical-align: top; padding: 8px" width="50%"&gt;&lt;span style="color:black; font-weight:bold"&gt;Supplier&lt;/span&gt;&lt;br /&gt;
			&amp;nbsp;
			&lt;table&gt;
				&lt;tbody&gt;
					&lt;tr&gt;
						&lt;td&gt;Company address&lt;/td&gt;
					&lt;/tr&gt;
				&lt;/tbody&gt;
			&lt;/table&gt;
			&lt;/td&gt;
			&lt;td style="text-align: left; vertical-align: top; padding: 8px" width="50%"&gt;&lt;span style="color:black; font-weight:bold"&gt;Customer &lt;/span&gt;&lt;br /&gt;

			&lt;p&gt;{%BillingAddress.ApplyTransformation("Ecommerce.Transformations.Order_Address") |(user)administrator|(hash)c314701fe021e9c2751e8bc0e6938764a1ccd82a07aa6e8fe935b5ebf5640dcd%}&lt;/p&gt;
			&lt;/td&gt;
		&lt;/tr&gt;
		&lt;tr&gt;
			&lt;td colspan="2"&gt;
			&lt;table width="100%"&gt;
				&lt;tbody&gt;
					&lt;tr&gt;
						&lt;td style="text-align: left; padding: 8px"&gt;&lt;span style="color:black; font-weight:bold"&gt;Payment option&lt;/span&gt;&lt;/td&gt;
						&lt;td style="text-align: left; padding: 8px"&gt;{%PaymentOption.PaymentOptionDisplayName |(user)administrator|(hash)ef1fb7f2da7a78564ef263c4332b9b5ea472748177e1b100bb9d92b9fdf2bf73%}&lt;/td&gt;
					&lt;/tr&gt;
				&lt;/tbody&gt;
			&lt;/table&gt;
			&lt;/td&gt;
		&lt;/tr&gt;
		&lt;tr&gt;
			&lt;td colspan="2" style="text-align: left; padding: 8px"&gt;{%ContentTable.ApplyTransformation("Ecommerce.Transformations.Order_ContentTable", "Ecommerce.Transformations.Order_ContentTableHeader", "Ecommerce.Transformations.Order_ContentTableFooter") |(user)administrator|(hash)df8feeed81de9dedc505cff8b44ddee8480ff298bbf9f380f46e54276883ef4a%} 
			&lt;hr size="1" /&gt;
			&lt;table cellpadding="5" width="50%" style="float: right;"&gt;
				&lt;tbody&gt;
					&lt;tr&gt;
						&lt;td&gt;Subtotal&lt;/td&gt;
						&lt;td style="text-align: right; padding-right: 0px"&gt;{% HTMLEncode((TotalItemsPrice).Format(Currency.CurrencyFormatString)) |(user)administrator|(hash)43dc3a801201bf16c39d2e43139b6855c7228a889801559b4d6013fab5c1be8f%}&lt;/td&gt;
					&lt;/tr&gt;
					&lt;tr&gt;
						&lt;td colspan="2" style="padding: 0; text-align: right;"&gt;{% OrderRelatedDiscountSummaryItems.Any() ? OrderRelatedDiscountSummaryItems.ApplyTransformation("Ecommerce.Transformations.Order_OrderRelatedDiscountsContent", "Ecommerce.Transformations.Order_OrderRelatedDiscountsHeader", "Ecommerce.Transformations.Order_OrderRelatedDiscountsFooter") : "" |(user)administrator|(hash)60308d8d17a71a8c1c41b0c1a6ec7bbf766d76fba7f350c3f1ad45354518b24c%}&lt;/td&gt;
					&lt;/tr&gt;
					&lt;tr&gt;
						&lt;td&gt;Shipping&lt;/td&gt;
						&lt;td style="text-align: right; padding-right: 0px"&gt;{% HTMLEncode(TotalShipping.Format(Currency.CurrencyFormatString)) |(user)administrator|(hash)abdd2a76777776b9bf2067ff950e29a2f2f9396457e93a156158080c37daca8e%}&lt;/td&gt;
					&lt;/tr&gt;
					&lt;tr&gt;
						&lt;td&gt;&lt;span style="color:black; font-weight:bold"&gt;TOTAL (incl. tax):&lt;/span&gt;&lt;/td&gt;
						&lt;td style="text-align: right; padding-right: 0px; color:black; font-weight:bold"&gt;{% HTMLEncode(TotalPrice.Format(Currency.CurrencyFormatString))|(user)administrator|(hash)0dacadd4fd1e18b32bdfea5d3f69d07dd8d9940283d6a821a87df9092b333adb%}&lt;/td&gt;
					&lt;/tr&gt;
				&lt;/tbody&gt;
			&lt;/table&gt;
			&lt;/td&gt;
		&lt;/tr&gt;
        &lt;tr&gt;
            &lt;td colspan="2"&gt;
            &lt;table width="100%"&gt;
                &lt;tbody&gt;
                    &lt;tr&gt;
                        &lt;td style="text-align: left;"&gt;&lt;span style="font-weight: bold;"&gt;Order note&lt;/span&gt;&lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td style="text-align: left;"&gt;{%HTMLENcode(Order.OrderNote)|(user)administrator|(hash)893863abcb6e5b5f7d60df9ed3585a2604107b7ea725ee9d21ba4915fd5813d6%}&lt;/td&gt;
                    &lt;/tr&gt;
                &lt;/tbody&gt;
            &lt;/table&gt;
            &lt;/td&gt;
        &lt;/tr&gt;
    &lt;/tbody&gt;
&lt;/table&gt;
&lt;/body&gt;
&lt;/html&gt;</EmailTemplateText>
      <EmailTemplateGUID>674d1b85-ce19-40bd-b2df-166a5891090a</EmailTemplateGUID>
      <EmailTemplateLastModified>2017-05-31T09:43:48.9676769+02:00</EmailTemplateLastModified>
      <EmailTemplateType>ecommerce</EmailTemplateType>
